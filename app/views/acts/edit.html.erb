      <div class="text-center" id="paginator">
        <%= paginate @acts, :remote => true, :theme => 'twitter-bootstrap-3', :window => 1%>
      </div>
    </div>
    <div id="act" class="well">
      <%= render 'act_form'%>
    </div>
  </div>
</div>